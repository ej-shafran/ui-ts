{
  "name": "ui-ts",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "homepage": "http://127.0.0.1",
  "scripts": {
    "build": "tsup src/** --dts --format esm,cjs --out-dir lib",
    "compile:watch": "tsc -w --noEmit",
    "prepublish": "npm run build",
    "pretty": "prettier . -w",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "types": "npx @arethetypeswrong/cli --pack",
    "test": "vitest",
    "spell": "cspell '**/*.*'"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./lib/*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts"
    }
  },
  "keywords": [],
  "author": "ej-shafran",
  "license": "ISC",
  "dependencies": {
    "fp-ts": "^2.16.1",
    "immer": "^10.0.2",
    "ts-adt": "^2.1.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.9.0",
    "@types/jsdom": "^21.1.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "cspell": "^7.3.2",
    "eslint": "^8.48.0",
    "fast-check": "^3.12.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
