{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "jsxFactory": "UI.elem",
    "jsxFragmentFactory": "UI.fragment",
    "module": "commonjs",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictBindCallApply": true
  }
}
