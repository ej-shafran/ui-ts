{
  "compilerOptions": {
    "noEmit": true,
    "outDir": "./dist/lib",
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    // "lib": [],
    "sourceMap": false,
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "skipLibCheck": true
  },
  "include": ["./src", "./test"],
  "exclude": ["./node_modules"]
}
